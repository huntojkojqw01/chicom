model = 'shozoku'

$('form#new_' + model).clear_previous_errors()

<% if @shozoku.errors.any? %>  
  $('form#new_' + model).render_form_errors(model, '<%=@shozoku.errors.messages.to_json.html_safe%>')
<% else %>
  $('#shozoku-new-modal').modal('hide'); 
  $('#shozokus').DataTable().row.add( [
    "<%=@shozoku.id%>",
    "<%=@shozoku.code%>",
    "<%=@shozoku.name%>"
  ]).draw( false );
<% end %>
